            android:layout_marginTop="15dp"
            android:layout_weight="1"
            android:background="@drawable/zygame_alert_dialog_right_selector_green"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="16sp" />
    </LinearLayout>

