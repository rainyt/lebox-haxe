                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_bule"
                android:text="关闭测试面板入口"
                android:textColor="@color/white" />
        </zygame.layout.FlowLayout>
    </ScrollView>

    <ListView
        android:id="@+id/listview"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:divider="#33333333"
        android:dividerHeight="0.6dp"
        android:fadeScrollbars="false"
        android:listSelector="#00000000"
        android:scrollbarFadeDuration="0"
        android:scrollbarSize="10dp"
        android:scrollbarThumbVertical="@drawable/zygame_scrollbar_style"
        android:scrollbarTrackVertical="@drawable/zygame_listview_scrollbar"
        android:visibility="visible" >
    </ListView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <Button
