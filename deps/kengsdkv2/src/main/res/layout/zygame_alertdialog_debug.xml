<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/lLayout_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/zygame_dialog_bg"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/txt_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:textColor="@color/black2"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/txt_msg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="15dp"
        android:gravity="center"
        android:lineSpacingMultiplier="1.2"
        android:textColor="@color/black2"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/edit_msg"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/zygame_dialog_edittext"
        android:cursorVisible="false"
        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
        android:gravity="center"
        android:hint="在此输入兑换码"
        android:inputType="number"
        android:lineSpacingMultiplier="1.2"
        android:maxLength="15"
        android:textColor="@color/black2"
        android:textSize="16sp" />

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:scrollbarSize="10dp"
        android:scrollbarThumbVertical="@drawable/zygame_scrollbar_style"
        android:scrollbarTrackVertical="@drawable/zygame_listview_scrollbar"
        android:visibility="visible" >

        <zygame.layout.FlowLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true" >

            <Button
                android:id="@+id/comment_log"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="log输出"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/get_appid"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="获取appid"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/change_float"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_bule"
                android:text="更改位置"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/show_auth"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="实名认证"
                android:textColor="@color/white"
                android:visibility="visible" />

            <Button
                android:id="@+id/show_healthytime1"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="时间段"
                android:textColor="@color/white"
                android:visibility="visible" />

            <Button
                android:id="@+id/show_healthytime2"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="时长1"
                android:textColor="@color/white"
                android:visibility="visible" />

            <Button
                android:id="@+id/show_healthytime3"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="时长2"
                android:textColor="@color/white"
                android:visibility="visible" />

            <Button
                android:id="@+id/pay_btn"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="支付功能"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/pay_btntest"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="测试支付"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/pay_moni"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="模拟支付"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/initad"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="当前渠道"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/shenhe"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="审核状态"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/sdklist"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="SDK列表"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/showinter"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="展示插屏"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/shoevideo"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="展示视频"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/shoebanner"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="展示横幅"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/closebanner"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="关闭横幅"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/shownative"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="展示原生"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/closenative"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="关闭原生"
                android:textColor="@color/white" />

            <Button
                android:id="@+id/servertime"
                android:layout_width="80dp"
                android:layout_height="40dp"
                android:background="@drawable/zygame_alert_dialog_right_selector_green"
                android:text="服务器时间"
                android:textColor="@color/white" />

            <Button
